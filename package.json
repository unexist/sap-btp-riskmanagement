{
  "name": "RiskManagement",
  "version": "1.0.0",
  "description": "RiskManagement project",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.4.0",
    "@sap-cloud-sdk/util": "^3.4.0",
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "@sap/low-code-event-handler": "^1.2.3",
    "@sap/mdk-tools": "^1.3.1",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^3.3.3",
    "express": "^4",
    "hdb": "^0.19.5",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/ux-specification": "^1.108.12"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-RiskManagement.Risks": "cds watch --open RiskManagement.Risks/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana"
      },
      "app-service": {
        "impl": "@sap/low-code-event-handler"
      },
      "auth": {
        "kind": "xsuaa"
      },
      "BusinessPartnerA2X": {
        "kind": "odata-v2",
        "model": "srv/external/BusinessPartnerA2X",
        "[production]": {
          "credentials": {
            "destination": "apihub_sandbox",
            "path": "s4hanacloud/sap/opu/odata/sap/API_BUSINESS_PARTNER"
          }
        }
      }
    },
    "features": {
      "fetch_csrf": true
    }
  },
  "lcap": true,
  "sapux": [
    "app/RiskManagement.Risks"
  ]
}
